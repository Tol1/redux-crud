{
  "name": "redux-crud",
  "version": "3.3.0",
  "description": "Redux CRUD",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc ava dist/**/*.test.js",
    "lint": "tslint src/**/*.ts",
    "format": "prettier --bracket-spacing=false --write 'src/**/*.ts'",
    "precommit": "lint-staged",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write --bracket-spacing=false",
      "git add"
    ]
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Versent/redux-crud"
  },
  "keywords": [
    "redux",
    "crud"
  ],
  "author": "Versent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Versent/redux-crud/issues"
  },
  "homepage": "https://github.com/Versent/redux-crud",
  "devDependencies": {
    "ava": "^0.22.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "nyc": "^13.1.0",
    "prettier": "^1.3.1",
    "testdouble": "^3.2.5",
    "tslint": "^5.7.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "invariant": "^2.1.0",
    "lodash.snakecase": "^4.1.1",
    "ramda": "^0.24.1"
  },
  "ava": {
    "failFast": true
  },
  "nyc": {
    "include": [
      "dist/**/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
